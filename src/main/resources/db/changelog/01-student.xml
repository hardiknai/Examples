<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
	<changeSet id="010" author="Hardik Nai">
		<createTable tableName="student"
			remarks="A table to contain all the student">
			<column name="id" type="bigint">
				<constraints primaryKey="true"
					primaryKeyName="pk_student_id" />
			</column>
			<column name="name" type="varchar(255)">
				<constraints nullable="false"
					primaryKeyName="nn_student_name" />
			</column>
			<column name="male" type="varchar(6)">
				<constraints nullable="false" />
			</column>
		</createTable>
	</changeSet>
	<changeSet id="011" author="Hardik Nai">
		<loadUpdateData encoding="UTF-8"
			file="db/changelog/data/school.csv" primaryKey="PRIMARY"
			separator=",," tableName="school" />
	</changeSet>

</databaseChangeLog>